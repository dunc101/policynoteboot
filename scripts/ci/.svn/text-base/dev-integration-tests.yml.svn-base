platform: linux
image_resource:
  type: docker-image
  source: {repository: maven, tag: 3.5-jdk-8}
inputs:
  - name: resource-dev-integration-code
run:
  path: sh
  args:
  - -exc
  - |
    mkdir ${HOME}/.m2
    cd resource-dev-integration-code
    cp settings.xml ${HOME}/.m2/
    ls -lart ${HOME}/.m2
    cd PolicyNoteServiceDomain
    mvn install -DskipTests=true